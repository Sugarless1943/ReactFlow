{
  "name": "umi-template",
  "version": "1.0.0",
  "private": true,
  "license": "Proprietary",
  "author": "YO-I",
  "scripts": {
    "build": "max build",
    "dev": "max dev",
    "format": "prettier --cache --write .",
    "gen-open-api": "npx openapi-generator-cli generate -g typescript-axios --additional-properties=withSeparateModelsAndApi=true,modelPackage=models,apiPackage=api -i http://localhost:8000/xinyegang/qms/v2/api-docs -o ./src/api/docs --skip-validate-spec",
    "postinstall": "max setup",
    "prepare": "husky install",
    "setup": "max setup",
    "start": "npm run dev"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-components": "^1.1.3",
    "@umijs/max": "^4.0.13",
    "ahooks": "^3.7.0",
    "antd": "^4.20.7",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "qs": "^6.11.0",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^2",
    "prettier-plugin-packagejson": "^2",
    "typescript": "^4.1.2"
  }
}
